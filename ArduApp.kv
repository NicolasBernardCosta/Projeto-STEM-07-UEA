<MyPopup@Popup>:
    auto_dismiss: False
    title: 'Alterar Variável Por'
    title_allign: 'center'
    size_hint_y:0.5
    size_hint_x:0.5
    MenuButton:
        text: 'Close me!'
        on_release: root.dismiss()

				

